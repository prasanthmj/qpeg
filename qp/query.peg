{
package qp
}

Field
  = src:Source _ "=" _ value:Identifier {
      return &Field{Key:src.(*Source), Value:value.(string)}, nil
  }

Source
  = name:Identifier path:("." Identifier)* {
      return makeSource(name,path)
  }
  
Identifier
  = [a-zA-Z0-9_]+ { 
      return string(c.text), nil
}

_
  = [ \t]*

  
EOF 
  = !.